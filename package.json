{
	"name": "melonpan-block-site-title",
	"version": "1.1.0",
	"description": "Melonpan Block Site Title WordPress Plugin",
	"homepage": "https://wordpress.org/plugins/melonpan-block-site-title/",
	"author": "Alvaro Garcia",
	"private": true,
	"scripts": {
		"dev": "webpack --mode development --watch --config ./webpack/build.config.babel.js",
		"prod": "webpack --mode production --config ./webpack/build.config.babel.js",
		"copy": "webpack --mode production --config ./webpack/copy.config.babel.js && rm _release/_temp.js",
		"version": "webpack --mode production --config ./webpack/version.config.babel.js && rm _temp.js",
		"release": "rm -rf build _release && npm run type && npm run version && npm run prod && npm run copy",
		"type": "tsc"
	},
	"devDependencies": {
		"@babel/core": "^7.9.0",
		"@babel/plugin-proposal-class-properties": "^7.8.3",
		"@babel/plugin-proposal-object-rest-spread": "^7.9.0",
		"@babel/preset-env": "^7.9.0",
		"@babel/preset-react": "^7.9.4",
		"@babel/preset-typescript": "^7.9.0",
		"@babel/register": "^7.9.0",
		"@types/lodash": "^4.14.149",
		"@types/node": "^13.11.0",
		"@types/react-dom": "^16.9.6",
		"@types/wordpress__api-fetch": "^3.2.2",
		"@types/wordpress__block-editor": "^2.2.6",
		"@types/wordpress__blocks": "^6.4.5",
		"@types/wordpress__components": "^8.5.2",
		"@types/wordpress__data": "^4.6.6",
		"@types/wordpress__element": "^2.4.0",
		"@types/wordpress__i18n": "^3.4.0",
		"babel-loader": "^8.1.0",
		"clean-webpack-plugin": "^3.0.0",
		"copy-webpack-plugin": "^5.1.1",
		"css-loader": "^3.5.0",
		"file-loader": "^6.0.0",
		"lodash": "^4.17.15",
		"mini-css-extract-plugin": "^0.9.0",
		"nib": "^1.1.2",
		"optimize-css-assets-webpack-plugin": "^5.0.3",
		"string-replace-loader": "^2.2.0",
		"stylus": "^0.54.7",
		"stylus-loader": "^3.0.2",
		"typescript": "^3.8.3",
		"webpack": "^4.42.1",
		"webpack-cli": "^3.3.11"
	}
}
